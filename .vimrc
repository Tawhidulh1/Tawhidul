execute pathogen#infect()
syntax on
filetype plugin indent on
colorscheme gruvbox
set background=dark
set tabstop=4
set shiftwidth=4
set softtabstop=4
set textwidth=79
set backspace=indent,eol,start
set nu
nnoremap <leader>n :NERDTreeFocus<CR>
nnoremap <C-n> :NERDTree<CR>
nnoremap <C-t> :NERDTreeToggle<CR>
nnoremap <C-f> :NERDTreeFind<CR>
